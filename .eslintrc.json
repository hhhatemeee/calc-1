{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "consistent-return": ["error", { "treatUndefinedAsUnspecified": true }],
        "import/newline-after-import": ["error", { "count": 2 }]
    }
}
